.container {
  height: 105vh;
  padding: 20px;
}
page {
  background-image: linear-gradient(90deg, #add8e67a 0%, #c2e59c31 100%);
}
.box {
  position: absolute;
  left: 0;
  right: 0;
  padding: 15rpx;
  border: 0rpx solid #efefef;
  border-radius: 8rpx;
  margin: 0rpx 15rpx 15rpx 15rpx;
  box-shadow:
  6px 6px 0px #add8e69f;
}
.tittle{
  padding: 15rpx;
  margin-top: 0rpx;
  margin-left: 0.5rem;
  font-size: larger;
  font-weight: bolder;
  padding-bottom: 1rem;
}
.message {
  margin-bottom: 0.7rem;
  padding-left: 2rem;
  font-weight: bold;
}
.message::after {
  content: "";
  display: block;
  border-bottom: solid 2px #a7d3e8;
  margin-top: 0.3rem;
}
.dtl{
  font-weight: 500;
}

.section {
  padding-top: 1rem;
  padding-left: 1.8rem;
  margin-bottom: 20rpx;
  border: 0rpx solid #efefef;
  border-radius: 8rpx;
  margin: 0rpx 15rpx 15rpx 15rpx;
  box-shadow:
  -6px -6px 0px #add8e69f;
}
.section2 {
  width: 80%;
  margin-left: -4rpx;
  margin-bottom: 20rpx;
}

.title {
  margin-left: -1rem;
  font-size: 18px;
  font-weight: bold;
  margin-bottom: 10px;
}

.content {
  font-size: 14px;
  margin-bottom: 10px;
}
.content2 {
  width: 100%;
  font-size: 14px;
  margin-bottom: 10px;
  padding-bottom: 1rem;
  padding-right: 1.8rem;
  border: 0rpx solid #efefef;
  border-radius: 8rpx;
  margin-bottom: 3rem;
  box-shadow:
  6px 6px 0px #add8e69f;
}

.member {
  margin-bottom: 5px;
}

.role {
  font-weight: bold;
}

.name {
  margin-left: 10px;
}
.name::after {
  content: "";
  display: block;
  border-bottom: solid 2px #a7d3e8;
  margin-top: 0.3rem;
}
